{
  "name": "sentigator",
  "version": "0.0.1",
  "description": "Content aggregator which searches for a given term in multiple services (Twitter, Google, UTube, etc.) and performs sentiment analysis on some of the results",
  "main": "sentigator.js",
  "scripts": {
     "test": "node ./node_modules/mocha/bin/mocha ./test"
  },
  "keywords": [
    "content",
    "aggregator",
    "sentiment",
    "analysis"
  ],
  "author": "Itai Koren (@itkoren)",
  "license": "MIT",
  "engines": {
    "node": ">= 0.10.x"
  },
  "dependencies": {
      "sentiment":"0.0.x",
      "request":"2.34.x",
      "async":"0.8.x",
      "JSONStream":"0.7.x",
      "event-stream":"3.1.x",
      "express": "4.1.x",
      "body-parser": "1.0.x",
      "cookie-parser": "1.1.x",
      "method-override": "1.0.x",
      "express-session": "1.1.x",
      "morgan": "1.0.x",
      "errorhandler": "1.0.x",
      "response-time": "1.0.x",
      "jade": "1.3.x",
      "sockjs":"0.3.x",
      "rbytes": "1.0.x"
  },
  "devDependencies": {
      "mocha":"1.19.x",
      "chai":"1.9.x",
      "sinon":"1.10.x",
      "mockery":"1.4.x",
      "supertest":"0.12.x"
  }
}
